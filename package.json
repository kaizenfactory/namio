{
  "name": "namio",
  "version": "0.0.1",
  "type": "module",
  "description": "Brandable name generator + .com checker (Attio-inspired)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolaiSchmid/namio.git"
  },
  "bugs": {
    "url": "https://github.com/NicolaiSchmid/namio/issues"
  },
  "homepage": "https://github.com/NicolaiSchmid/namio#readme",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "namio": "src/cli.ts"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/cli.ts",
    "generate": "bun run src/cli.ts generate",
    "check": "bun run src/cli.ts check",
    "hunt": "bun run src/cli.ts hunt",
    "build": "bun build src/cli.ts --outfile dist/namio.js",
    "compile": "bun build --compile src/cli.ts --outfile dist/namio"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.6.3"
  }
}
